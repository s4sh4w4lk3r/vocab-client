{
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false,
    "singleAttributePerLine": true,
    "bracketSameLine": false,
    "endOfLine": "lf",

    "quoteProps": "preserve",
    "proseWrap": "preserve"
}
